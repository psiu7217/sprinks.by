<div class="baner_block">
    {% for banner in banners %}
        <div class="baner_item">
            {% if banner.link %}
                <a href="{{ banner.link }}">
                    <img src="{{ banner.image }}" alt="{{ banner.title }}"/>
                </a>
            {% else %}
                <img src="{{ banner.image }}" alt="{{ banner.title }}"/>
            {% endif %}
        </div>
    {% endfor %}
</div>
